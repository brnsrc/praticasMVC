@model IEnumerable<praticasMVC.Models.Movie>

<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
    </head>
    <body>
        <h1>@ViewBag.Title</h1>
        @foreach (var item in Model)
        {
            <div>
                <h2>@item.Nome</h2>
                <h3>@item.Descricao</h3>
                <h3>@item.DataLancamento</h3>
            </div>
        }
    </body>
</html>